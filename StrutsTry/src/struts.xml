<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC 
	".//Apache Software Foundation//DTD Struts Confguration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true"></constant>
	<package name="login" namespace="/" extends="struts-default">
<!-- 		<interceptors> -->
<!-- 			<interceptor-stack name="logStack"> -->
<!-- 				<interceptor-ref name="defaultStack"/> -->
<!-- 				<interceptor-ref name="logger"/> -->
<!-- 			</interceptor-stack> -->
<!-- 		</interceptors> -->
		<action name="login" class="tutorial.getMetod.LoginClass">
			<interceptor-ref name="logStack"/>
			<result name="success">/search.jsp</result>
			<result name="login">/login.jsp</result>
			<result name="input">/login.jsp</result>
		</action>
	</package>

	<package name="default" namespace="/tutorial" extends="struts-default">
		<action name="tutorial" class="tutorial.getMetod.TryAction">
			<result name="success">/success.jsp</result>
			<result name="input">/search.jsp</result>
		</action>
	</package>
	
	
</struts>